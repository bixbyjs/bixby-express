{
  "name": "bixby-express",
  "version": "0.2.3",
  "description": "Express application components for Bixby.js.",
  "keywords": [
    "express",
    "http"
  ],
  "author": {
    "name": "Jared Hanson",
    "email": "jaredhanson@gmail.com",
    "url": "https://www.jaredhanson.me/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/bixbyjs/bixby-express.git"
  },
  "bugs": {
    "url": "https://github.com/bixbyjs/bixby-express/issues"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/jaredhanson"
  },
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://opensource.org/licenses/MIT"
    }
  ],
  "main": "./lib",
  "assembly": {
    "namespace": "http",
    "components": [
      "main",
      "middleware/authenticate",
      "middleware/cleanstate",
      "middleware/csrfprotection",
      "middleware/errorlogging",
      "middleware/parsecookies",
      "middleware/state",
      "state/store",
      "session/stores/memory",
      "session/stores/redis",
      "auth/authenticator"
    ]
  },
  "directories": {
    "com": "./app"
  },
  "dependencies": {
    "connect-redis": "^5.0.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.1",
    "csurf": "^1.10.0",
    "express": "^4.17.1",
    "express-session": "^1.16.1",
    "flowstate": "^0.5.0",
    "fluidfactory": "^0.1.0",
    "morgan": "^1.8.2",
    "passport": "^0.4.0",
    "utils-merge": "^1.0.1"
  },
  "devDependencies": {
    "make-node": "0.3.x",
    "proxyquire": "^2.1.3",
    "mocha": "^6.2.1",
    "chai": "^4.2.0",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.3.0",
    "redis": "^3.0.2"
  },
  "engines": {
    "node": "*"
  },
  "scripts": {
    "test": "node_modules/.bin/mocha --reporter spec --require test/bootstrap/node test/*.test.js"
  }
}
