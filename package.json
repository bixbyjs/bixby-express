{
  "name": "bixby-express",
  "version": "0.2.1",
  "description": "Express application components for Bixby.js.",
  "keywords": [
    "components",
    "express",
    "http"
  ],
  "author": {
    "name": "Jared Hanson",
    "email": "jaredhanson@gmail.com",
    "url": "http://www.jaredhanson.net/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/bixbyjs/bixby-express.git"
  },
  "bugs": {
    "url": "http://github.com/bixbyjs/bixby-express/issues"
  },
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "http://opensource.org/licenses/MIT"
    }
  ],
  "main": "./app",
  "assembly": {
    "namespace": "http",
    "components": [
      "main",
      "middleware/authenticate",
      "middleware/ceremony",
      "middleware/cors",
      "middleware/csrfprotection",
      "middleware/errorlogging",
      "middleware/initialize",
      "middleware/logging",
      "middleware/parse",
      "middleware/parsecookies",
      "middleware/session",
      "session/store/redis/discover",
      "session/store/redis/store",
      "state/store"
    ]
  },
  "dependencies": {
    "body-parser": "^1.17.1",
    "connect-redis": "^3.4.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.1",
    "csurf": "^1.9.0",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "fluidfactory": "^0.1.0",
    "morgan": "^1.8.2",
    "passport": "^0.4.0",
    "utils-merge": "^1.0.1"
  },
  "devDependencies": {
    "make-node": "0.3.x",
    "mocha": "2.x.x",
    "chai": "3.x.x",
    "proxyquire": "~1.0.1"
  },
  "engines": {
    "node": "*"
  },
  "scripts": {
    "test": "node_modules/.bin/mocha --reporter spec --require test/bootstrap/node test/*.test.js"
  }
}
